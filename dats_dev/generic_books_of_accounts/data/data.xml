<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="0">
        <record id="books_sales_sequence" model="ir.sequence">
            <field name="name">Sales Book Sequence</field>
            <field name="code">books.sales.sequence</field>
            <field name="prefix">S%(year)s-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="books_purchase_sequence" model="ir.sequence">
            <field name="name">Purchase Book Sequence</field>
            <field name="code">books.purchase.sequence</field>
            <field name="prefix">P%(year)s-</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>

</odoo>

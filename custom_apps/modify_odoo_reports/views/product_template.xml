<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="product_template_tree_view_modify_odoo_reports" model="ir.ui.view">
        <field name="name">product.template.product.template.tree.view.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_tree_view"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="inside">
                <header>
                    <button string="Print Product Count sheet" name ="print_product_count_sheet" type="object" class="oe_highlight"/>
                </header>
            </xpath>
        </field>
    </record>

</odoo>
